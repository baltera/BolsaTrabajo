<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff61697a">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/tvTablaGradoEspecializacion"
        android:id="@+id/TVtablaGradoEspecializacion"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:textColor="#ffd25a56"
        android:theme="@android:style/Animation"
        android:textSize="27dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/insertarImgBoton"
        android:onClick="insertarGradoEspecializacion"
        android:src="@drawable/botoninsertar"
        android:layout_gravity="center_horizontal"
        android:layout_below="@+id/TVtablaGradoEspecializacion"
        android:layout_alignLeft="@+id/modificarImgBoton"
        android:layout_alignStart="@+id/modificarImgBoton"
        android:layout_marginTop="92dp"
        android:background="#ff61697a" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/eliminarImgBoton"
        android:onClick="eliminarGradoEspecializacion"
        android:src="@drawable/botoneliminar"
        android:layout_gravity="center_horizontal"
        android:layout_below="@+id/insertarImgBoton"
        android:layout_alignLeft="@+id/insertarImgBoton"
        android:layout_alignStart="@+id/insertarImgBoton"
        android:background="#ff61697a" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/modificarImgBoton"
        android:onClick="modificarGradoEspecializacion"
        android:src="@drawable/botonmodificar"
        android:adjustViewBounds="true"
        android:layout_gravity="center_horizontal"
        android:layout_below="@+id/eliminarImgBoton"
        android:layout_centerHorizontal="true"
        android:background="#ff61697a" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/consultarImgBoton"
        android:onClick="consultarGradoEspecializacion"
        android:src="@drawable/botonconsultar"
        android:layout_gravity="center_horizontal"
        android:layout_below="@+id/modificarImgBoton"
        android:layout_centerHorizontal="true"
        android:background="#ff61697a" />

</RelativeLayout>
